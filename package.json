{
  "name": "trainingmug",
  "displayName": "TrainingMug",
  "description": "Event responder",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vsinder-sidebar-view",
          "title": "VSinder",
          "icon": "media/heart.svg"
        }
      ]
    },
    "views": {
      "vsinder-sidebar-view": [
        {
          "type": "webview",
          "id": "vsinder-sidebar",
          "name": "VSinder",
          "icon": "media/heart.svg",
          "contextualTitle": "VSinder"
        }
      ]
    },
    "commands": [
      {
        "command": "trainingmug.main",
        "category": "TrainingMug",
        "title": "Main"
      },
      {
        "command": "trainingmug.askQuestion",
        "category": "TrainingMug",
        "title": "Ask Question"
      },
      {
        "command": "trainingmug.refresh",
        "category": "TrainingMug",
        "title": "Refresh"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup --bundleConfigAsCjs -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@tsconfig/svelte": "^5.0.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "concurrently": "^8.2.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "rollup": "^3.26.3",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  }
}
